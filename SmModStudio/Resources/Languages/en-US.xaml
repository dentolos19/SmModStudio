    <!-- Dialog Strings -->

    <sys:String x:Key="TxtDialogTitle">SmModStudio</sys:String>

    <sys:String x:Key="TxtDialogMsg0">Make sure all the inputs are filled!</sys:String>
    <sys:String x:Key="TxtDialogMsg1">The selected path doesn't contain the game's executable!</sys:String>
    <sys:String x:Key="TxtDialogMsg2">All settings has been saved! Would you like to restart this app?</sys:String>
    <sys:String x:Key="TxtDialogMsg3">Are you sure that you want to reset to default settings?</sys:String>
    <sys:String x:Key="TxtDialogMsg4">Select a valid mod to work with to continue.</sys:String>
    <sys:String x:Key="TxtDialogMsg5">This feature is currently unavailable.</sys:String>
    <sys:String x:Key="TxtDialogMsg6">You must enter a file name to continue!</sys:String>
    <sys:String x:Key="TxtDialogMsg7">File already exists with that name!</sys:String>
    <sys:String x:Key="TxtDialogMsg8">Are you sure that you want to delete this file/folder?</sys:String>

    <!-- Other Strings -->

    <sys:String x:Key="TxtOtherMsg0">No mods found</sys:String>
    <sys:String x:Key="TxtOtherMsg1">The error report doesn't contain stack trace info.</sys:String>

</ResourceDictionary>