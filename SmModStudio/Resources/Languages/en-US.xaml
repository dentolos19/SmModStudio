    <sys:String x:Key="TxtErrorTitle">A fatal error had occurred internally!</sys:String>
    <sys:String x:Key="TxtErrorStackTrace">Stack Trace</sys:String>
    <sys:String x:Key="TxtErrorMessage">Message</sys:String>

    <!-- Sub-Windows Strings -->

    <sys:String x:Key="TxtFileName">File Name</sys:String>
