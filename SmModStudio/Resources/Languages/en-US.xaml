    <sys:String x:Key="TxtCreate">Create</sys:String>
    <sys:String x:Key="TxtBrowse">Browse</sys:String>
    <sys:String x:Key="TxtYes">Yes</sys:String>
    <sys:String x:Key="TxtNo">No</sys:String>

    <!-- Window Title Strings -->

    <sys:String x:Key="TxtStudioTitle">Scrap Mechanic Mod Studio</sys:String>
    <sys:String x:Key="TxtOpenProjectTitle">Open Project</sys:String>
    <sys:String x:Key="TxtErrorHandlerTitle">Error Handler</sys:String>
